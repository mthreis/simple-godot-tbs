[sub_resource type="GDScript" id=4]

script/source = "extends Node2D

var s
var tilemap

func _ready():
	tilemap = get_parent()
	s = get_node(\"../cyborg\")
	set_process(true)

func _process(dt):
	var mousePos = tilemap.world_to_map(get_global_mouse_pos())
	
	if tilemap.is_withinv(mousePos) && tilemap.get_cellv(mousePos) >= 0:
		s.set_pos(tilemap.map_to_world(mousePos))
		
		var height = tilemap.get_heightmap_at_v(mousePos)
		
		s.get_node(\"sprite\").set_pos(Vector2(0, -height))"

